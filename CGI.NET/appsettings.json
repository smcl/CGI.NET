{
  "CgiBin": [
    {
      "Method": "GET",
      "Route": "cgi-bin/hello",
      "Command": "python",
      "Arguments": "-c \"print('hello from python')\""
    },

    {
      "Method": "POST",
      "Route": "cgi-bin/hey",
      "Command": "python",
      "Arguments": "-c \"print('hey ' + input()  + ' from python')\""
    }
  ],

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
